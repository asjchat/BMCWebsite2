          "_name": "apricing1",
          "_sourceTheme": "urgem5",
          "_customHTML": "<section class=\"pricing1\" group=\"Pricing\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"checkbox\" name=\"reverseContent\" title=\"Reverse\" condition=\"showBlurredCircle\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Blurred Circle\" name=\"showBlurredCircle\" checked>\n        <input type=\"color\" name=\"circleColor1\" title=\"Circle Color 1\" value=\"#000000\" condition=\"showBlurredCircle\">\n        <input type=\"color\" name=\"circleColor2\" title=\"Circle Color 2\" value=\"#000000\" condition=\"showBlurredCircle\">\n        <input type=\"color\" name=\"circleColor3\" title=\"Circle Color 3\" value=\"#656565\" condition=\"showBlurredCircle\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Content Bg\" name=\"showContentBg\" checked>\n        <input type=\"color\" name=\"contentBgColor\" title=\"Content Bg Color\" value=\"#161616\" condition=\"showContentBg\">\n        <input type=\"checkbox\" title=\"Price\" name=\"showPrice\" checked>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showCardText\" checked>\n        <input type=\"checkbox\" title=\"Card Comment\" name=\"showComment\" checked>\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#ffffff\" selected>\n        <input type=\"color\" title=\"Icon BG Color\" name=\"iconBgColor\" value=\"#353535\" selected>\n        <input type=\"range\" inline title=\"Column\" name=\"columnCount\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Text Item\" name=\"textCount\" min=\"0\" max=\"5\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Icon Item\" name=\"iconCount\" min=\"0\" max=\"5\" step=\"1\" value=\"5\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/urgem5/components/_images/background2.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/urgem5/components/_images/background2.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#d8d5d1\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"circle-blur\" mbr-if=\"showBlurredCircle\"></div>\n    <div class=\"container\">\n        <div class=\"row justify-content-center\" mbr-if=\"showTitle || showSubtitle\">\n            <div class=\"col-12\">\n                <div class=\"title-container\">\n                    <h3 class=\"mbr-section-title mb-0 mbr-fonts-style align-center\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                        <b>Pricing</b></h3>\n                    <h4 class=\"mbr-section-subtitle mb-0 mbr-fonts-style align-center\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">Compared to similar programs, BMC provides more value at a lower price.&nbsp;</h4>\n                </div>\n            </div>\n        </div>\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12\">\n                <div class=\"content-container\">\n\n                    <div class=\"card-head grid-row-item\">\n                        <div class=\"head-clear\"></div>\n                        <div class=\"head-titles grid-column-item\">\n                            <div class=\"head-item head-item-1\">\n                                <p class=\"card-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-name\"><b>Black Mountain College</b></p>\n                                <div class=\"price-container\" mbr-if=\"showPrice\">\n                                    <p class=\"price-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".price-text\">at</p>\n                                    <p class=\"price-number mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".price-number\"><b>$5000</b></p>\n                                </div>\n                                <p class=\"card-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">for Summer Program (with scholarship opportunities)</p>\n                            </div>\n\n                            <div class=\"head-item head-item-2\" mbr-if=\"columnCount >1\">\n                                <p class=\"card-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-name\"><b>Launch X</b></p>\n                                <div class=\"price-container\" mbr-if=\"showPrice\">\n                                    <p class=\"price-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".price-text\">at</p>\n                                    <p class=\"price-number mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".price-number\">\n                                        <b>$9,450</b></p>\n                                </div>\n                                <p class=\"card-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">for Summer Program</p>\n                            </div>\n                            <div class=\"head-item head-item-3\" mbr-if=\"columnCount >2\">\n                                <p class=\"card-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-name\"><b>Harvard SSP</b></p>\n                                <div class=\"price-container\" mbr-if=\"showPrice\">\n                                    <p class=\"price-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".price-text\">at</p>\n                                    <p class=\"price-number mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".price-number\">\n                                        <b>$13,750</b>\n                                    </p>\n                                </div>\n                                <p class=\"card-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">for Summer Program</p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card-item grid-row-item\" mbr-if=\"textCount >0\">\n                        <div class=\"item-name-container\">\n                            <p class=\"item-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-name\"><b>Access to Network of Mentors</b></p>\n                        </div>\n                        <div class=\"card-list grid-column-item\">\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\">fdf</p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >1\" data-app-selector=\".item-text\">\n                                Medium - High\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >2\" data-app-selector=\".item-text\">\n                                High\n                            </p>\n                        </div>\n                    </div>\n                    <div class=\"card-item grid-row-item\" mbr-if=\"textCount >1\">\n                        <div class=\"item-name-container\">\n                            <p class=\"item-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-name\"><b>Internship Opportunities After Graduation</b></p>\n                        </div>\n                        <div class=\"card-list grid-column-item\">\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\">\n                                Low - Medium\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >1\" data-app-selector=\".item-text\">\n                                Medium - High\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >2\" data-app-selector=\".item-text\">\n                                High\n                            </p>\n                        </div>\n                    </div>\n                    <div class=\"card-item grid-row-item\" mbr-if=\"textCount >2\">\n                        <div class=\"item-name-container\">\n                            <p class=\"item-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-name\">\n                                <b>ANIMATION COMPLEXITY</b>\n                            </p>\n                        </div>\n                        <div class=\"card-list grid-column-item\">\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\">\n                                Low - Medium\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >1\" data-app-selector=\".item-text\">\n                                Medium - High\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >2\" data-app-selector=\".item-text\">\n                                High\n                            </p>\n                        </div>\n                    </div>\n                    <div class=\"card-item grid-row-item\" mbr-if=\"textCount >3\">\n                        <div class=\"item-name-container\">\n                            <p class=\"item-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-name\">\n                                <b>ANIMATION COMPLEXITY</b>\n                            </p>\n                        </div>\n                        <div class=\"card-list grid-column-item\">\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\">\n                                Low - Medium\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >1\" data-app-selector=\".item-text\">\n                                Medium - High\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >2\" data-app-selector=\".item-text\">\n                                High\n                            </p>\n                        </div>\n                    </div>\n                    <div class=\"card-item grid-row-item\" mbr-if=\"textCount >4\">\n                        <div class=\"item-name-container\">\n                            <p class=\"item-name mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-name\">\n                                <b>ANIMATION COMPLEXITY</b>\n                            </p>\n                        </div>\n                        <div class=\"card-list grid-column-item\">\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\">\n                                Low - Medium\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >1\" data-app-selector=\".item-text\">\n                                Medium - High\n                            </p>\n                            <p class=\"item-text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"columnCount >2\" data-app-selector=\".item-text\">\n                                High\n                            </p>\n                        </div>\n                    </div>\n\n                    <div class=\"card-item grid-row-item\" mbr-if=\"iconCount >0\">\n                        <div class=\"item-name-container\">\n                            <p class